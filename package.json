{
  "name": "advanpro-epc-tools",
  "version": "1.0.0",
  "description": "Open tools for the devices of Advanpro Ltd",
  "scripts": {
    "pack:publish": "build",
    "build": "npm run pack && build",
    "build:win": "build --win --ia32 --x64",
    "build:mac": "npm run pack && build --mac",
    "build:linux": "npm run pack && build --linux",
    "dev": "npm run compile && node tasks/runner.js",
    "e2e": "npm run pack && mocha test/e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter app test",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix app test",
    "pack": "npm run pack:renderer && npm run pack:main",
    "compile": "cross-env BABEL_ENV=main NODE_ENV=development babel app/src/main -d app/dist  --source-maps",
    "compile:renderer": "cross-env NODE_ENV=development webpack -p --progress --colors --config webpack.renderer.config.js",
    "pack:main": "cross-env BABEL_ENV=main NODE_ENV=production babel app/src/main -d app/dist  --source-maps",
    "pack:renderer": "cross-env NODE_ENV=production webpack -p --progress --colors --config webpack.renderer.config.js",
    "test:app": "npm run compile && cross-env BABEL_ENV=main electron-mocha app/test/**/*.js --compilers js:babel-core/register -t 15000",
    "unit": "karma start test/unit/karma.conf.js",
    "dev:server": "babel src -d lib --source-maps && electron ./app/src/main/app.js"
  },
  "build": {
    "publish": {
      "provider": "generic",
      "url": "http://admin.dev.unifitapp.com/api/1.0/electron/${arch}/open_tools/"
    },
    "productName": "advanpro-epc-tools",
    "appId": "com.electron.advanpro-open-tool",
    "electronVersion": "1.7.5",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json",
      "config.js"
    ],
    "mac": {
      "icon": "app/icons/icon.icns",
      "CSC_LINK": "",
      "CSC_KEY_PASSWORD": "123456",
      "CSC_IDENTITY_AUTO_DISCOVERY": true,
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "app/icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    },
    "nsis": {
      "perMachine": true,
      "oneClick": false,
      "runAfterFinish": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "author": "Advanpro Ltd",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-node5": "^12.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^3.5.0",
    "cross-env": "^5.1.0",
    "css-loader": "^0.26.1",
    "del": "^2.2.1",
    "devtron": "^1.1.0",
    "electron": "1.7.5",
    "electron-builder": "^11.4.4",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-mocha": "^4.0.3",
    "electron-rebuild": "^1.1.3",
    "electron-reload": "^1.2.2",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-flowtype-errors": "^3.3.4",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "grunt": "^1.0.1",
    "grunt-electron-installer": "^2.1.0",
    "html-webpack-plugin": "^2.16.1",
    "inject-loader": "^2.0.1",
    "json-loader": "^0.5.4",
    "karma": "^1.7.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^2.0.5",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.5.3",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.5.2",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.5",
    "scss-loader": "^0.0.1",
    "should": "^11.2.1",
    "spectron": "^3.4.0",
    "style-loader": "^0.13.1",
    "supertest": "^3.0.0",
    "supertest-as-promised": "^4.0.2",
    "tree-kill": "^1.2.0",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.2.0",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^2.4.0"
  },
  "nodeSassConfig": {
    "binarySite": "https://npm.taobao.org/mirrors/node-sass"
  },
  "dependencies": {}
}
